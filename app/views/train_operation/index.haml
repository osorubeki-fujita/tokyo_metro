= render "common_content"

- content_for :right_contents do
  = @twitter_processor.render
  = @real_time_info_processor.render_meta_datum( include_train_location_infos: false )
  = ::TokyoMetro::App::Renderer::RealTimeInfos::SideMenu::LinksToRailwayLinePages.new( request ).render

= ::Train::Operation::InfoDecorator.render_top_title( request , domain_id_name: :train_operation_info_title )
= @real_time_info_processor.render
= ::TokyoMetro::App::Renderer::StationMatrix.new( request , @railway_line_infos , make_link_to_railway_line: true , controller_of_linked_page: :train_operation ).render
= ::TokyoMetro::App::Renderer::StationLinkList.new( request , @tokyo_metro_station_dictionary_including_main_info , :train_operation ).render
